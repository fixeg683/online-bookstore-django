{
    "version": 2,
    "builds": [
      {
        "src": "bookshop/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bookshop/wsgi.py"
      }
    ]
  }
  